{
	"extends": "../../tsconfig.json",
	"compilerOptions": {
		"composite": true,
		"experimentalDecorators": true,
		"emitDecoratorMetadata": true,
		"outDir": "./build/",
		"baseUrl": "src",
		"lib": ["ES2023"],
		"paths": {
			"Controllers/*": ["./Controllers/*"],
			"Middlewares/*": ["./Middlewares/*"],
			"Repositories/*": ["./Repositories/*"],
			"Routes/*": ["./Routes/*"],
			"Services/*": ["./Services/*"],
			"Types/*": ["./Types/*"],
			"Utils/*": ["./Utils/*"],
			"Errors/*": ["./Errors/*"],
			"Validations/*": ["./Validations/*"],
			"Versions/*": ["./Versions/*"],
			"@asset-management/common": ["../common"],
			"PrismaGenerated/*": ["../prisma/generated/*"]
		}
	},
	"include": ["src", "prisma", "prisma.config.ts"]
}
